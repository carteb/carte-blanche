    "node": "5.5.0"
  },
  "scripts": {
    "postinstall": "(cd plugins/react && npm install && cd ../../webpack-plugin && npm install && cd .. && npm run client:build)",
    "start": "npm run example:dev",
    "server": "webpack-dev-server --hot",
    "example:dev": "npm run server -- --context examples/dev/ --config examples/dev/webpack.dev.babel.js",
    "example:dev:iron": "iron-node node_modules/webpack-dev-server/bin/webpack-dev-server.js --context examples/dev/ --config examples/dev/webpack.dev.babel.js",
    "example:dest": "npm run server -- --context examples/dest/ --config examples/dest/webpack.dev.babel.js",
    "example:dest:iron": "iron-node node_modules/webpack-dev-server/bin/webpack-dev-server.js --context examples/dest/ --config examples/dest/webpack.dev.babel.js",
    "example:profile": "npm run server -- --context examples/profile/ --config examples/profile/webpack.dev.babel.js",
    "example:users": "npm run server -- --context examples/users/ --config examples/users/webpack.dev.babel.js",
    "example:radium": "npm run server -- --context examples/styling/radium/ --config examples/styling/radium/webpack.dev.babel.js",
    "example:jss": "npm run server -- --context examples/styling/jss/ --config examples/styling/jss/webpack.dev.babel.js",
    "example:aphrodite": "npm run server -- --context examples/styling/aphrodite/ --config examples/styling/aphrodite/webpack.dev.babel.js",
    "example:express": "(cd examples/express && npm start)",
    "example:commons": "npm run server -- --context examples/commons/ --config examples/commons/webpack.dev.babel.js",
    "example:redux:todomvc": "npm run server -- --context examples/redux/todomvc/ --config examples/redux/todomvc/webpack.config.babel.js",
    "formEnvironment": "npm run server -- --context plugins/react/formEnvironment/ --config plugins/react/formEnvironment/webpack.dev.babel.js",
    "client:dev": "webpack-dev-server --hot --config client/webpack.dev.babel.js",
    "client:build": "webpack --config client/webpack.prod.babel.js --progress -p",
    "plugin:react:dev": "(cd plugins/react/ && npm run dev)",
    "plugin:react:build": "(cd plugins/react/ && npm run build)",
    "build": "npm run client:build",
    "lint": "npm run lint:eslint && npm run lint:flow",
    "lint:eslint": "eslint ./ --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix ./ --ignore-path .gitignore",
    "lint:flow": "flow status",
    "test": "node_modules/.bin/mocha --opts mocha.opts",
    "test:watch": "npm test -- --watch",
    "test:coverage": "istanbul cover --config istanbul.yml _mocha -- --opts mocha.opts"
  },
